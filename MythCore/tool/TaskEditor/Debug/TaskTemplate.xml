      <TextNode Name="前置任务ID"/>
    </Option>
  </CondNode>
  <CondNode Name="完成条件">
    <Option Name="等级" Value="1">
      <TextNode Name="接受等级"/>
      <TextNode Name="显示等级"/>
    </Option>
  </CondNode>
  <DiagNode Name="接任务对话">
    <Option Name="NPC对话" Value="1">
      <TextNode Name="内容"/>
    </Option>
    <Option Name="玩家对话" Value="2">
      <TextNode Name="内容"/>
    </Option>
    <Option Name="男玩家对话" Value="3">
      <TextNode Name="内容"/>
    </Option>
    <Option Name="女玩家对话" Value="4">
      <TextNode Name="内容"/>
    </Option>
  </DiagNode>
