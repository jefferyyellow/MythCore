      <ParamNode Name="内容" Type="STRING256" EditType="EDITSTRING"/>
    </TypeNode>
    <TypeNode Name="玩家对话" Type="2" EditType="NONE">
      <ParamNode Name="内容" Type="STRING256" EditType="EDITSTRING"/>
    </TypeNode>
    <TypeNode Name="男玩家对话" Type="3" EditType="NONE">
      <ParamNode Name="内容" Type="STRING256" EditType="EDITSTRING"/>
    </TypeNode>
        <TypeNode Name="女玩家对话" Type="3" EditType="NONE">
      <ParamNode Name="内容" Type="STRING256" EditType="EDITSTRING"/>
    </TypeNode>
  </Node>
</Task>
